{
  "name": "mija",
  "version": "1.1.0",
  "description": "Console Logger with Emoji support!",
  "main": "dist",
  "scripts": {
		"build": "del dist && tsc",
		"test": "xo && npm run build && ava",
		"prepublishOnly": "npm run build"
	},
  "keywords": [
    "mija",
    "logger",
    "console",
    "log",
    "emoji",
    "color"
  ],
  "dependencies": {
    "chalk": "2.4.2"
  },
  "devDependencies": {
    "@akepinski/tsconfig": "0.0.2",
    "@types/execa": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "ava": "*",
    "del-cli": "^1.1.0",
    "eslint-config-xo-typescript": "^0.10.1",
    "execa": "^1.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^4.1.2",
    "xo": "*"
  },
  "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
  "bugs": {
    "url": "https://github.com/xxczaki/mija/issues"
  },
  "repository": "xxczaki/mija",
  "homepage": "https://github.com/xxczaki/mija#readme",
  "license": "MIT",
  "types": "dist",
	"sideEffects": false,
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"ava/no-ignored-test-files": "off",
			"new-cap": "off"
		},
		"ignores": [
			"test/fixture.js"
		]
	}
}
